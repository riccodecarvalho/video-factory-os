{
    "extends": [
        "next/core-web-vitals"
    ],
    "rules": {
        "prefer-const": "warn",
        "no-console": [
            "warn",
            {
                "allow": [
                    "warn",
                    "error",
                    "log"
                ]
            }
        ]
    },
    "ignorePatterns": [
        "node_modules/",
        ".next/",
        "artifacts/",
        "z- tmp/",
        "z- archive/",
        "scripts/",
        "lib/engine/ffmpeg.ts"
    ]
}